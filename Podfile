# Uncomment the next line to define a global platform for your project
 platform :ios, '7.0'

target 'FrameworkLY' do
  pod "AFNetworking"#, #"~> 2.0" #网络请求
  pod "SVProgressHUD" #hud
  pod "MJRefresh" #下拉/上拉
  pod "YTKNetwork" #网络层
  pod "MJExtension" #json转换为实体
  pod "YYKit" #是一组庞大、功能丰富的 iOS 组件
  pod 'NetworkEye'#, #'~> 1.0.1' #网络调试
  pod 'BlocksKit'#一组控件的block分类
  pod 'JPushSDK', '~> 1.8.2'#极光推送
  
  
  target 'FrameworkLYTests' do
    inherit! :search_paths
    # Pods for testing
  end

  target 'FrameworkLYUITests' do
    inherit! :search_paths
    # Pods for testing
  end

end
